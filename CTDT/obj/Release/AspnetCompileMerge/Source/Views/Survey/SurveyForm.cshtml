@using CTDT.Helper
@{
    ViewBag.Title = "Biểu mẫu đã khảo sát - TDMU";
    Layout = "~/Views/Shared/_Layout.cshtml";
    var user = SessionHelper.GetUser();
}
<div style="padding-top: 50px; padding-bottom: 70px;">
    <div class="container">
        <div class="row">
            <div class="col-md-12 columns">
                <div class="row justify-content-center">
                    <div class="col-md-12 text-center">
                        <h2 class="section-title">
                            DANH SÁCH PHIẾU ĐÃ KHẢO SÁT
                        </h2>
                    </div>
                    <div id="showdata" class="row justify-content-center">
                        <!--Load Dữ liệu-->
                        <input type="hidden" id="surveyIds" value="@string.Join(",", ViewBag.surveyIds)" />
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
@Scripts.Render("~/bundles/User/SurveyForm")